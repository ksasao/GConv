    <PublishSingleFile>false</PublishSingleFile>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>ksasao</Authors>
    <Company>ksasao</Company>
    <PackageProjectUrl>https://github.com/ksasao/GConv</PackageProjectUrl>
